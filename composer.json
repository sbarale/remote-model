{
  "name": "deelbaarmechelen/remote-model",
  "description": "An eloquent-like model, for the Laravel framework.",
  "keywords": ["laravel","eloquent","model","api","remote data","custom models"],
  "license": "BSD-2-Clause",
  "authors": [
    {
      "name": "Daniel Stainback",
      "email": "torann@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.5.9",
    "illuminate/support": "~5.5",
    "illuminate/contracts": "~5.5",
    "jenssegers/date": "^3.0",
    "illuminate/pagination": "~5.5"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.0",
    "mockery/mockery": "^0.9.4"
  },
  "autoload": {
    "psr-4": {
      "Torann\\RemoteModel\\": "src/Torann/RemoteModel"
    }
  },
  "autoload-dev": {
    "classmap": ["tests/stubs"]
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.2-dev"
    }
  },
  "config": {
    "preferred-install": "dist",
    "platform": {
      "php": "7.1.2"
    }
  }
}
